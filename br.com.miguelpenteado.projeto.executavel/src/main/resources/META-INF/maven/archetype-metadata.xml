<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="libStaticLibrary"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet encoding="UTF-8">
      <directory>src/main/cpp/__artifactId__</directory>
      <includes>
        <include>**/*.cpp</include>
        <include>**/*.hpp</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/cpp/header</directory>
      <includes>
        <include>**/*.hpp</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
